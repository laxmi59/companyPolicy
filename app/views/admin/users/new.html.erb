<h1>Admin::Users#new</h1>
<div class="main_content">
  <%= form_for @user, url: admin_users_path do |f| %>
    <fieldset class="inputs">
      <ol>
        <li>
          <%= f.label 'Email' %><br>
          <%= f.text_field :email %>
        </li>
      </ol>
    </fieldset>
    <fieldset class="actions">
      <ol>
        <li class="action input_action">
          <%= f.submit 'Submit' %>
        </li>
        <li class="cancel"><%= link_to "Cancel", admin_users_path %></li>
      </ol>
    </fieldset>
    <% end %>
</div>
